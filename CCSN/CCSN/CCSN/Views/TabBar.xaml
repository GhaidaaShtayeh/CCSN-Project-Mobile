    <CCSN:PatientsListPage Title ="Patients" BackgroundColor="White"  IconImageSource="https://upload.wikimedia.org/wikipedia/commons/thumb/c/cb/Noun_Project_people_icon_3376085.svg/1024px-Noun_Project_people_icon_3376085.svg.png"></CCSN:PatientsListPage>
    <CCSN:AppointmentPage Title ="Appointment" BackgroundColor="White"  IconImageSource="https://cdn2.iconfinder.com/data/icons/calendar-35/50/5-512.png"></CCSN:AppointmentPage>

</TabbedPage>